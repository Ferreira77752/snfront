programa {
  funcao inicio() {
    algoritmo "Fatorial de um Número"

    numero, fatorial, i: inteiro

    escreva("Digite um número inteiro positivo: ")
    leia(numero)
    fatorial := 1
    para i de 1 ate numero faca
        fatorial := fatorial * i

    escreva("O fatorial é: ", fatorial)


  }
}
